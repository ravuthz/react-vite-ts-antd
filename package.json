{
  "name": "react-antd-ts",
  "version": "0.0.1",
  "description": "A NPM Package use to shared Ant Design component with react typescript",
  "private": false,
  "scripts": {
    "dev": "vite",
    "clean": "rimraf ./dist",
    "copy-files": "copyfiles -u 1 ./src/**/*.css ./dist && cp package.json README.md ./dist",
    "build-lib": "npm run clean && tsc && npm run copy-files",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:wat": "vitest",
    "test:cov": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ravuthz/react-antd-ts.git"
  },
  "keywords": [
    "npm",
    "js",
    "ts",
    "javascript",
    "react",
    "typescript",
    "antd",
    "ant",
    "design"
  ],
  "author": "Vannaravuth Yo <ravuthz@gmail.com> (https://ravuthz.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ravuthz/react-antd-ts/issues"
  },
  "main": "dist/index.js",
  "homepage": "https://github.com/ravuthz/react-antd-ts#readme",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.21.4",
    "jotai": "^1.7.3",
    "less": "^4.1.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.14.0",
    "@testing-library/react": "^13.3.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-test-renderer": "^18.0.0",
    "c8": "^7.11.3",
    "copyfiles": "^2.4.1",
    "jsdom": "^20.0.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vite-plugin-imp": "^2.2.0",
    "vite-preset-react": "^2.3.0",
    "vitest": "^0.16.0"
  }
}
